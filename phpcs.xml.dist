<?xml version="1.0"?>
<ruleset name="cache">
    <description>The coding standard of cache package</description>
	<file>src</file>
	<file>tests</file>
    <arg value="p" />

    <config name="ignore_warnings_on_exit" value="1" />
    <config name="ignore_errors_on_exit" value="1" />

    <arg name="colors" />
    <arg value="s" />

    <!-- Use the PSR12 Standard-->
    <rule ref="PSR12" />
	<rule ref="Generic.Arrays.DisallowLongArraySyntax"/>
</ruleset>
